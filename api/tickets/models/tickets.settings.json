        "available",
        "reserved",
        "paid"
      ],
      "default": "available",
      "required": true
    },
    "number": {
      "type": "integer",
      "required": true,
      "unique": true,
      "max": 999,
      "min": 0
    },
    "rifa": {
      "model": "rifa",
      "via": "rifa_tickets"
    },
    "gambler": {
      "model": "gambler",
      "via": "gambler_tickets"
    }
  }
}
